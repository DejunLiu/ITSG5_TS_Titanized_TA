  ELSRH.srcPosVector := f_LongPosVector2ExtLongPosVector(LSRH.srcPosVector);
  ELSRH.gnAddress    := f_GN_Address2ExtGN_Address(LSRH.gnAddress);

  return ELSRH;
}
